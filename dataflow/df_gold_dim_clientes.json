{
	"name": "df_gold_dim_clientes",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_clientes",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_gold_dim_clientes",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          cst_id as integer,",
				"          cst_firstname as string,",
				"          cst_lastname as string,",
				"          cst_marital_status as string,",
				"          cst_gndr as string,",
				"          BDATE as date,",
				"          cntry as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT DISTINCT cst_id, cst_firstname, cst_lastname, cst_marital_status, cst_gndr, BDATE, cntry FROM SILVER.CLIENTES',",
				"     format: 'query') ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_cliente as integer,",
				"          cst_id as string,",
				"          firstname as string,",
				"          lastname as string,",
				"          marital_status as string,",
				"          gender as string,",
				"          fecha_nacimiento as date,",
				"          clocation as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          cst_id,",
				"          firstname = cst_firstname,",
				"          lastname = cst_lastname,",
				"          marital_status = cst_marital_status,",
				"          gender = cst_gndr,",
				"          fecha_nacimiento = BDATE,",
				"          clocation = cntry",
				"     )) ~> sink1"
			]
		}
	}
}